    <BuildMacro Include="iberbarSrcDir">
      <Value>$(iberbarSrcDir)</Value>
    </BuildMacro>
    <BuildMacro Include="BuildOutputDir">
      <Value>$(BuildOutputDir)</Value>
    </BuildMacro>
  </ItemGroup>
</Project>