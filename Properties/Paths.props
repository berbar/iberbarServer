    <iberbarSrcDir>$(SolutionDir)Src</iberbarSrcDir>
    <BuildOutputDir>$(SolutionDir)Bin\$(OSTarget)\$(Configuration)\$(PlatformTarget)</BuildOutputDir>
  </PropertyGroup>
  <PropertyGroup />
  <ItemDefinitionGroup />
